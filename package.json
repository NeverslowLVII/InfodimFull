{
  "name": "infodim",
  "version": "1.0.0",
  "scripts": {
    "install:frontend": "cd InfoDIMFrontend && npm install",
    "install:backend": "cd InfoDIMBackend && npm install",
    "build:frontend": "cd InfoDIMFrontend && npm run build",
    "build:backend": "cd InfoDIMBackend && tsc",
    "start:frontend": "cd InfoDIMFrontend && npm run preview",
    "start:backend": "cd InfoDIMBackend && npm run server",
    "build": "npm run build:frontend && npm run build:backend",
    "start": "npm run start:backend & npm run start:frontend"
  }
}
